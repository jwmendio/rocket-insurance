{
  "extends": [
      "plugin:react/recommended",
      "airbnb"
  ],
  "plugins": [
      "react",
      "simple-import-sort"
  ],
  "rules": {
    "simple-import-sort/sort": "error",
    "sort-imports": "off",
    "import/order": "off",
    "import/first": "error",
    "import/newline-after-import": "error",
    "import/no-duplicates": "error",
    "radix": "off",
    "react/jsx-filename-extension": "off",
    "no-undef": "off",
    "no-console": "off"
  }
}
